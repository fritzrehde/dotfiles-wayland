profile Laptop {
  output eDP-1 mode 1920x1080@60Hz scale 1.2 position 0,0
}

profile Workplace1440p {
  output HDMI-A-1 mode 2560x1440 position 0,0
  output eDP-1 mode 1920x1080@60Hz scale 1.2 position 480,1440
}

profile Workplace1080p {
  output HDMI-A-1 mode 1920x1080 position 0,0
  output eDP-1 mode 1920x1080@60Hz scale 1.2 position 160,1080
}
